
# SET Game Detector Configuration
# Copy this file to .env and update with your EC2 server endpoint

# EC2 Server Endpoint (replace with your EC2 instance's public DNS or IP)
REACT_APP_EC2_SERVER_URL=http://your-ec2-instance-public-dns:8000/detect-sets

# Set to 'false' to use the actual EC2 backend instead of mock data
REACT_APP_USE_MOCK_DATA=false
